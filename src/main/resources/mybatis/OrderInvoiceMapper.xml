<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.single.pro.mapper.OrderInvoiceDao">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.single.pro.entity.OrderInvoice">
        <id column="id" property="id" />
        <result column="order_no" property="orderNo" />
        <result column="company_name" property="companyName" />
        <result column="company_tax_no" property="companyTaxNo" />
        <result column="invoice_cost" property="invoiceCost" />
        <result column="email" property="email" />
        <result column="remarks" property="remarks" />
        <result column="status" property="status" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

</mapper>
