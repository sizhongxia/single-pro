<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.single.pro.mapper.WorkerCashLogDao">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.single.pro.entity.WorkerCashLog">
        <id column="id" property="id" />
        <result column="cash_cost" property="cashCost" />
        <result column="serial_number" property="serialNumber" />
        <result column="cash_status" property="cashStatus" />
        <result column="cash_remarks" property="cashRemarks" />
        <result column="worker_id" property="workerId" />
        <result column="create_time" property="createTime" />
        <result column="handle_time" property="handleTime" />
    </resultMap>

</mapper>
