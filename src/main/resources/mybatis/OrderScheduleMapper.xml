        <result column="days" property="days" />
        <result column="worker_status" property="workerStatus" />
        <result column="work_content" property="workContent" />
        <result column="ack_status" property="ackStatus" />
        <result column="ack_time" property="ackTime" />
        <result column="comment" property="comment" />
        <result column="ack_customer_id" property="ackCustomerId" />
