        <result column="to_user_id" property="toUserId" />
        <result column="ser_attitude_score" property="serAttitudeScore" />
        <result column="ser_efficiency_score" property="serEfficiencyScore" />
        <result column="ser_quality_score" property="serQualityScore" />
        <result column="ser_comment" property="serComment" />
        <result column="from_user_id" property="fromUserId" />
