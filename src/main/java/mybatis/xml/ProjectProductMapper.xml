        <result column="number" property="number" />
        <result column="ser_survey_choice" property="serSurveyChoice" />
        <result column="ser_check_choice" property="serCheckChoice" />
        <result column="ser_construct_choice" property="serConstructChoice" />
        <result column="ser_train_choice" property="serTrainChoice" />
        <result column="ser_accept_choice" property="serAcceptChoice" />
        <result column="remarks" property="remarks" />
