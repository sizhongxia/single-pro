<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.single.pro.mapper.ProductDao">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.single.pro.entity.Product">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="kind_id" property="kindId" />
        <result column="type_id" property="typeId" />
        <result column="company_id" property="companyId" />
        <result column="model" property="model" />
        <result column="describe" property="describe" />
        <result column="constituent" property="constituent" />
        <result column="survey_items" property="surveyItems" />
        <result column="install_debug" property="installDebug" />
        <result column="device_maintain" property="deviceMaintain" />
        <result column="common_problem" property="commonProblem" />
        <result column="doc_url" property="docUrl" />
        <result column="manual_url" property="manualUrl" />
        <result column="apply_user_id" property="applyUserId" />
        <result column="show_status" property="showStatus" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

</mapper>
