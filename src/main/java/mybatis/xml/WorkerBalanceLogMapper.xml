<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.single.pro.mapper.WorkerBalanceLogDao">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.single.pro.entity.WorkerBalanceLog">
        <id column="id" property="id" />
        <result column="worker_id" property="workerId" />
        <result column="order_no" property="orderNo" />
        <result column="bef_balance" property="befBalance" />
        <result column="change_money" property="changeMoney" />
        <result column="aft_balance" property="aftBalance" />
        <result column="status" property="status" />
        <result column="remarks" property="remarks" />
        <result column="create_time" property="createTime" />
        <result column="handle_time" property="handleTime" />
    </resultMap>

</mapper>
