<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.single.pro.mapper.UserIdentityCardDao">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.single.pro.entity.UserIdentityCard">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="identity_card" property="identityCard" />
        <result column="identity_card_front_pic" property="identityCardFrontPic" />
        <result column="identity_card_back_pic" property="identityCardBackPic" />
        <result column="upload_time" property="uploadTime" />
        <result column="audit_state" property="auditState" />
        <result column="audit_time" property="auditTime" />
    </resultMap>

</mapper>
